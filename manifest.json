{
    "manifest_version": 2,
    "name": "CSS JS Insertr",
    "version": "0.1.0",
    "description": "Allows you to add custom JavaScript/CSS to any website.",
    "permissions": [
        "storage"
    ],
    "browser_action": {},
    "permissions": ["activeTab", "storage"],
    "background": {
        "scripts": ["app.js"]
    },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["insert.js"]
        }
    ]
}
